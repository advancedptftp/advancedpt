                                    <div>
                                        <div class="row form-group">
                                            <div class="col-sm-6 p_top10">Patient’s MC#:</div>
                                            <div class="col-sm-6">@Html.TextBoxFor(m => m.MCPreApproval.PatientMCNo, new { @class = "form-control", @disabled = "disabled" })</div>
                                        </div>
                                        <div class="row form-group">
                                            <div class="col-sm-6">Check Driver’s License Region C:</div>
                                            <div class="col-sm-6">
                                                <label class="radio-inline">@Html.RadioButtonFor(m => m.MCPreApproval.IsRegionC, Model.MCPreApproval.IsRegionC, new { @disabled = "disabled" }) Yes</label>
                                                <label class="radio-inline">@Html.RadioButtonFor(m => m.MCPreApproval.IsRegionC, !Model.MCPreApproval.IsRegionC, new { @disabled = "disabled" }) No</label>
