                                            @Html.RadioButtonFor(m => m.KneeBraceCustomForm.Gender, "M", new { @class = "radio-custom Present_Intensity_Symptom", @id = "Male", @disabled = "disabled" })

                                            <label for="Male" class="radio-custom-label">Male</label>

