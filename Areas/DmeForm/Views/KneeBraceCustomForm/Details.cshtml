                                        <div class="col-sm-5 p_top10"><strong>4. Knee Width:</strong></div>
                                        <div class="col-sm-7">

                                            <div class="form-group">
                                                @Html.TextBoxFor(m => m.KneeBraceCustomForm.Fitting_KneeWidth, new { @class = "form-control shortfield", @readonly = "readonly" })
