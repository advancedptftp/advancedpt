                                        <li>Fitting & Adjustment</li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-sm-3" style="margin-top: 30px;"><img src="@Url.Content("~/Images/pic1.png")" alt="" /></div>
                        </div>

