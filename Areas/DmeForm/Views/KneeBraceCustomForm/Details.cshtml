                                        <div class="col-sm-4 radiosec">
                                            @Html.RadioButtonFor(m => m.KneeBraceCustomForm.Gender, "F", new { @class = "radio-custom Present_Intensity_Symptom", @id = "Female", @disabled = "disabled" })

                                            <label for="Female" class="radio-custom-label">Female</label>

                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>

                      
                    </div>


