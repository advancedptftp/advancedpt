                                    <label for="KneeBraceCustomForm_IsCustomOALateralOffLoader_01300" class="checkbox-custom-label">
                                        <span id="Desc_2">01300</span>
                                        <span id="Desc_22">Custom OA Lateral Off-Loader</span>
                                    </label>
                                </div>
