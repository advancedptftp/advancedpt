                                                <br />
                                                @Html.ValidationMessageFor(model => model.KneeBraceCustomForm.Fitting_KneeOffset)
                                                <span id="Fitting_KneeOffset" class="validateMsg"></span>
                                            </div>
                                        </div>
                                    </div>
