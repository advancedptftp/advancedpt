                                                <br />
                                                @Html.ValidationMessageFor(model => model.KneeBraceCustomForm.Fitting_KneeWidth)
                                                <span id="Fitting_KneeWidth" class="validateMsg"></span>
                                            </div>

                                        </div>
                                    </div>

                                </div>

                            </div>

                        </div>

                        
                    </div>

                    <div id="div_4">
