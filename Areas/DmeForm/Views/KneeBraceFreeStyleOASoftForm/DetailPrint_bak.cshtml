                    <div style="padding:0 0 5px 10px;">@Html.CheckBoxFor(e => e.Paperwork.IsDeliveryTicketComplete, new { @class = "checkbox-inline", @disabled = "disabled" })Delivery Ticket complete, pt signs, give pt carbon copy</div>
                    <div style="padding:0 0 5px 10px;">@Html.CheckBoxFor(e => e.Paperwork.IsAOBComplete, new { @class = "checkbox-inline", @disabled = "disabled" })AOB complete, pt signs, give pt carbon copy</div>
                    <div style="padding:0 0 5px 10px;">@Html.CheckBoxFor(e => e.Paperwork.IsCheckListComplete, new { @class = "checkbox-inline", @disabled = "disabled" })Check List complete with staff initials, pt signs, keep in chart</div>
