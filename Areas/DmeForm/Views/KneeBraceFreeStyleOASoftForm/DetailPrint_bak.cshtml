                    <div style="padding:0 0 5px 10px;">@Html.CheckBoxFor(e => e.Paperwork.IsHIPPA2GiveToPT, new { @class = "checkbox-inline", @disabled = "disabled" })HIPPA #2 give to pt</div>
                    <div style="padding:0 0 5px 10px;">@Html.CheckBoxFor(e => e.Paperwork.IsProtocolComplaintsGiveToPT, new { @class = "checkbox-inline", @disabled = "disabled" })Protocol for Complaints give to pt</div>
                    <div style="padding:0 0 5px 10px;">@Html.CheckBoxFor(e => e.Paperwork.IsMC_DMEPOS_Supplier_StandardsGiveToPT, new { @class = "checkbox-inline", @disabled = "disabled" })MC DMEPOS Supplier Standards give to pt</div>
