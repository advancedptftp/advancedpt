                        <table width="100%" border="0">
                            <tr>
                                <td width="24%">MC Rep Name</td>
                                <td width="76%">
                                    <div style="padding:3px 0 3px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">
                                        @Model.MCPreApproval.MCRep_Name
                                </div>
                            </td>
                        </tr>
                    </table>

                </div>
                <div style="padding:5px 0 0 0;">
                    <table width="100%" border="0">
                        <tr>
                            <td width="19%">MC Rep Badge#:</td>
                            <td width="34%"><div style="padding:3px 0 3px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">@Model.MCPreApproval.MCRep_BadgeNo</div></td>
                            <td width="7%" style="padding-left:15px;">Int</td>
                            <td width="40%"><div style="padding:3px 0 3px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">@Model.MCPreApproval.MCRep_Initial</div></td>
                        </tr>
                    </table>

                </div>*@
                    <div style="padding:5px 0 0 0;">
                        <table width="100%" border="0">
                            <tr>
                                <td width="60%">Same or similar device issued in the past 5 years?</td>
                                <td width="39%">@Html.RadioButtonFor(m => m.MCPreApproval.IsSimilarDeviceIssued, true, new { @disabled = "disabled" })Yes&nbsp;&nbsp;@Html.RadioButtonFor(m => m.MCPreApproval.IsSimilarDeviceIssued, false, new { @disabled = "disabled" })No&nbsp;&nbsp;</td>
                            </tr>
                        </table>

                    </div>
            </div>
        </div>
            <div style="padding:10px; border:2px solid #000; margin-top:10px;">
