                                                    @Html.HiddenFor(e => e.DMEDiognosis[i].DMEDiagnosis_Id, new { @class = "icdidForICD", @disabled = "disabled" })
                                                </div>*@
                                                </div>
                                                <script>
                                                    counter += 1;
                                                </script>
                                            }

                                        </div>
                                    </div>
                                    @*<div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;"></div>
                                <div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;"></div>*@

                                </td>
                                <td width="31%">
                                    ICD-9 Code<br>
                                    <div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;"></div>
                                    @*<div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;"></div>
                                <div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;"></div>*@

                                </td>
                            </tr>
                        </table>

                    </div>
                    <div style="padding:5px 0 0 0;">
                        <table width="100%" border="0">
                            <tr>
                                <td width="69%" style="padding-right:15px;">
                                    <input type="checkbox">Item Requested By:<br>
                                    <div style="padding:7px 0 7px 0;">
                                        <table width="100%" border="0">
                                            <tr>
                                                <td width="19%">
                                                    @Html.CheckBoxFor(e => e.Prescription.IsItemReqByTherapist, new { @class = "", @disabled = "disabled" })
                                                    Therapist
                                                </td>
                                                <td width="81%"><div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">@Model.Prescription.Therapist_Name</div></td>
                                            </tr>
                                        </table>

                                    </div>
                                    <div style="padding:7px 0 7px 0;">
                                        <table width="100%" border="0">
                                            <tr>
                                                <td width="15%">
                                                    @Html.CheckBoxFor(e => e.Prescription.IsItemReqByPatient, new { @class = "", @disabled = "disabled" })
                                                    Patient
                                                </td>
                                                <td width="11%">
                                                    @Html.CheckBoxFor(e => e.Prescription.IsItemReqByDoctor, new { @class = "", @disabled = "disabled" })
                                                    Doctor
                                                </td>
                                                <td width="74%"><div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">@Model.Prescription.Doctor_Name</div></td>
                                            </tr>
                                        </table>

                                    </div>

                                </td>

                            </tr>
                        </table>

                    </div>
                    <div style="padding:5px 0 0 0;">
                        <table width="100%" border="0">
                            <tr>
                                <td width="69%" style="padding-right:15px;">
                                    <div>
                                        <table width="100%" border="0">
                                            <tr>
                                                <td width="77%">
                                                    @Html.CheckBoxFor(e => e.Prescription.IsCompleteLOMN, new { @class = "", @disabled = "disabled" })
                                                    Complete <strong> of Medical Necessity</strong>
                                                </td>
                                                <td width="5%">Int</td>
                                                <td width="18%"><div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">@Model.Prescription.CompleteLOMN_Initial</div></td>
                                            </tr>
                                        </table>
                                    </div>


                                </td>
                            </tr>
                        </table>

                    </div>
                    <div style="padding:5px 0 0 0;">
                        <table width="100%" border="0">
                            <tr>
                                <td width="69%" style="padding-right:15px;">
                                    <div>
                                        <table width="100%" border="0">
                                            <tr>
                                                <td width="77%">
                                                    @Html.CheckBoxFor(e => e.Prescription.IsGiveFormAndLOMN, new { @class = "", @disabled = "disabled" })
                                                    Give FORM &amp; LETTER of Med.Nec. to Office Staff
                                                </td>
                                            </tr>
                                        </table>
                                    </div>


                                </td>
                            </tr>
                        </table>

                    </div>
                </div>

            </div>
            <div style="padding:10px; border:2px solid #000; margin-top:10px;">
                <h3 style="padding:0 0 5px 0; margin:0;">3. Office Staff <span style="font-weight:normal; font-style:italic;">order</span></h3>

                <div style="padding:0 0 20px 0;">
                    <div style="padding:0px 0 7px 0;">
                        <table width="100%" border="0">
                            <tr>
                                <td width="58%">
                                    @Html.CheckBoxFor(e => e.Order.IsFaxLOMN, new { @class = "checkbox-inline", @disabled = "disabled" })
                                    Fax Letter of Med.Nec.
                                </td>
                                <td width="8%">Date</td>
                                <td width="34%">
                                    <div style="padding:7px 0 7px 0; border-bottom:1px solid #333; margin-bottom:0px; min-height:18px;">
                                        @*@Html.TextBoxFor(m => m.Order.FaxLOMN_Date, new { @class = "form-control  datepicker-bg", style = "width:100%;", @Readonly = true })*@
                                        @Model.Order.FaxLOMN_Date
                                    </div>
                                </td>
                            </tr>
                        </table>

                    </div>
                    <div style="padding:0px 0 7px 0;">
                        <table width="100%" border="0">
                            <tr>
                                <td width="58%">
                                    @Html.CheckBoxFor(e => e.Order.IsReceiveLOMN, new { @class = "checkbox-inline", @disabled = "disabled" })
