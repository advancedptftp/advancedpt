                    $('#SpinomedIVTLSOForm_Model_No').val(modelNo);
                    $('#SpinomedIVTLSOForm_TLSOFittingSize_Id').val(size).attr("selected", "selected");
                    $($("#tbl_TLSO_Fitting tbody tr")[3]).css("color", "#f58f22");
                }
                //X-Large	24-26.5 inches
