                    $('#SpinomedIVTLSOForm_TLSOFittingSize_Id').val(size).attr("selected", "selected");
                    $($("#tbl_TLSO_Fitting tbody tr")[4]).css("color", "#f58f22");
                }
                else
                {
                    alert('Sacrum to Just Below C7 not in range!.......');
                }
            }
            else {
