<script type="text/javascript">
    function fn_SetModelNo() {
        var size = "";
        var inches = "";
        var modelNo = "";

        inches = $('#SpinomedIVTLSOForm_TLSOFitting_SacrumBelowC7').val();
        $("#tbl_TLSO_Fitting tbody tr").css("color", "");
        //alert(inches);
        if (inches != undefined) {
            if ((inches != null) && (inches != '') && (inches != ' '))
            {
                //alert(inches);
                //X-Small	Up To 17 inches	
                if (inches >= 1 && inches <= 17)
                {
                    modelNo = 85021;
                    size = 1;
                    $('#SpinomedIVTLSOForm_Model_No').val(modelNo);
                    $('#SpinomedIVTLSOForm_TLSOFittingSize_Id').val(size).attr("selected", "selected");
                    $($("#tbl_TLSO_Fitting tbody tr")[0]).css("color", "#f58f22");
                }
                 //Small	17-19.5 inches
                else if (inches >= 17 && inches <= 19.5) {
                    modelNo = 85022;
                    size = 2;
                    $('#SpinomedIVTLSOForm_Model_No').val(modelNo);
                    $('#SpinomedIVTLSOForm_TLSOFittingSize_Id').val(size).attr("selected", "selected");
                    $($("#tbl_TLSO_Fitting tbody tr")[1]).css("color", "#f58f22");
                }
                //Medium	19.5-21.75 inches
                else if (inches >= 19.5 && inches <= 21.75)
                {
                    modelNo = 85023;
                    size = 3;
                    $('#SpinomedIVTLSOForm_Model_No').val(modelNo);
                    $('#SpinomedIVTLSOForm_TLSOFittingSize_Id').val(size).attr("selected", "selected");
                    $($("#tbl_TLSO_Fitting tbody tr")[2]).css("color", "#f58f22");
                }
                    //Large	   21.75-24 inches
                else if (inches >= 21.75 && inches <= 24)
                {
