    function validateInputs() {
        var flag = true;
        var Model = document.getElementById('SpinomedIVTLSOForm_Model_No');
        var Patient_Name = document.getElementById("SpinomedIVTLSOForm_Patient_Name");
        var DOB = document.getElementById("SpinomedIVTLSOForm_DOB");
        var Therapist = document.getElementById("Prescription_Therapist_Name");
        var Doctor = document.getElementById("Prescription_Doctor_Name");
        var Initial = document.getElementById("Prescription_CompleteLOMN_Initial");
        var Diagnosis_Desc = document.getElementById("DMEDiognosis_0__Diagnosis_Desc");
        var ICD10Code = document.getElementById("DMEDiognosis_0__ICD10Code");

        if (Model.value == "" || Model.value ==null ||Model.value =='') {
            $("#Model_No").show();
            $("#Model_No").text("Enter Size");
            Model.focus();
            flag = false;
        }
        else {
            $("#Model_No").hide();
            $("#Model_No").text("");
        }

        if (Patient_Name.value == "" || Patient_Name.value == '' || Patient_Name.value == null) {
            $("#Patient_Name").show();
            $("#Patient_Name").text("Enter Patient Name");
            Patient_Name.focus();
            flag = false;
        }
        else
        {
            $("#Patient_Name").hide();
            $("#Patient_Name").text("");
        }

        if (DOB.value == "" ||DOB.value ==null ||DOB.value =='') {
            $("#DOB").show();
            $("#DOB").text("Enter DOB");
            DOB.focus();
            flag = false;
        }
        else{
            $("#DOB").hide();
            $("#DOB").text("");
        }
         if ((Math.abs(new Date().getFullYear() - new Date(DOB.value).getFullYear())) < 10) {
            $("#DOB").show();
            $("#DOB").text("Patient's age should be 10 years or more!!");
            DOB.focus();
            flag = false;
         }

         if (Diagnosis_Desc.value != undefined) {
             if (Diagnosis_Desc.value == null || Diagnosis_Desc.value == "" || Diagnosis_Desc.value == '') {
                 $("#Diagnosis_Desc").show();
                 $("#Diagnosis_Desc").text("Enter Diagnosis_Desc");
                 Diagnosis_Desc.focus();
                 flag = false;
             }
             else {
                 $("#Diagnosis_Desc").hide();
                 $("#Diagnosis_Desc").text("");
             }
         }

         if (ICD10Code.value != undefined) {
             if (ICD10Code.value == null || ICD10Code.value == "" || ICD10Code.value == '') {
                 $("#ICD10Code").show();
                 $("#ICD10Code").text("Enter ICD10 Code");
                 ICD10Code.focus();
                 flag = false;
             }
             else {
                 $("#ICD10Code").hide();
                 $("#ICD10Code").text("");
             }
         }

       
         if (Initial.value == "" || Initial.value == null || Initial.value == '') {
            $("#CompleteLOMN_Initial").show();
            $("#CompleteLOMN_Initial").text("Enter Initial");
            Initial.focus();
            flag = false;
         }
         else {
             $("#CompleteLOMN_Initial").hide();
             $("#CompleteLOMN_Initial").text("");
         }
        
        if ($("#Prescription_IsItemReqByTherapist").is(':checked')) {
            if (Therapist.value == "") {
                $("#Therapist_Name").show();
                $("#Therapist_Name").text("Enter Therapist Name");
                Therapist.focus();
                flag = false;
            }
        } else if ($("#Prescription_IsItemReqByDoctor").is(':checked')) {
            if (Doctor.value == "") {
                $("#Doctor_Name").show();
                $("#Doctor_Name").text("Enter Doctor Name");
                Doctor.focus();
                flag = false;

            }

        }

        if (Initial.value == "" || Initial.value == null || Initial.value == "") {
            $("#Initial").show();
            $("#Initial").text("Enter Initial");
            Therapist.focus();
            flag = false;
        }
        else {
            $("#Initial").hide();
            $("#Initial").text("");
        }

        if (
            $("#Prescription_IsItemReqByPatient").is(':checked')) {
            if ((Doctor.value == "") && (Therapist.value == "")) {
                return true;
            }
        }
        else if ((Doctor.value == "") && (Therapist.value == "")) {
            $("#Therapist_Name").show();
            $("#Therapist_Name").text("Enter Therapist Name");
            Therapist.focus();
            $("#Doctor_Name").show();
            $("#Doctor_Name").text("Enter Doctor Name");
            Doctor.focus();
            flag = false;
        }

        return flag;

    }
</script>