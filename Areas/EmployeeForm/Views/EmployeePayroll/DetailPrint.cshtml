@model DataLayer.Models.tblTEmployeePayroll

@{
    Layout = null;
}
@using DataLayer;

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>myadvancedpt.com</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <link rel="shortcut icon" href="favicon.png">

    <!-- Bootstrap core CSS -->


</head>

<body>


    <div style="width:700px; margin:0 auto; padding:5px 0; font-family:verdana; font-size:13px; color:#333;">


        <div style="padding:0 0 20px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="38%">
                        <h2 style="padding:0; font-size:20px; margin:0;">
                            Employee Payroll<br>
                            <span style="font-size:15px; font-weight:normal;">Heal At Home Healthcare</span>
                        </h2>
                    </td>
                    <td width="62%"><div style="padding:10px; text-align:center; border:2px dashed #333; font-size:13px; font-weight:bold;">CONFIDENTIAL REPORT: SUBMIT ORIGINAL TO THE DIRECTOR OF NURSING WITHIN 24 HOURS OF INCIDENT. DO NOT SCAN OR COPY.</div></td>
                </tr>
            </table>

        </div>


        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="31%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">
                          @ViewBag.Location
                        </div>
                        Office Locaton
                    </td>

                    <td width="31%" style="padding-right:10px;" valign="top"></td>
                </tr>
            </table>

        </div>
        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="31%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.EmpPayroll_Id</div>
                        Employee ID
                    </td>

                    <td width="31%" style="padding-right:10px;" valign="top">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Today_date.GetExactValue()</div>
                        Today Date
                    </td>
                </tr>
            </table>

        </div>
        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="31%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Emp_firstname</div>
                        Employee First Name
                    </td>

                    <td width="31%" style="padding-right:10px;" valign="top">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Emp_lastname</div>
                        Employee Last Name
                    </td>
                </tr>
            </table>

        </div>
        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="31%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Payroll_startdate.GetExactValue()</div>
                        Payroll Start Date
                    </td>

                    <td width="31%" style="padding-right:10px;" valign="top">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Payroll_Enddate.GetExactValue()</div>
                        Payroll End date
                    </td>
                </tr>
            </table>

        </div>
        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="72%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">
                            @if (Model.Request_paidtime)
                            { <span>Yes</span>
                            }
                            else
                            {<span>No</span> }
                        </div>
                        Request for payment of Paid Time off(PTO) Hours
                    </td>

                    <td width="28%" style="padding-right:10px;" valign="top">
                        @if (Model.Request_paidtime)
                        {
                            <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Hours_Requested_Paidtime.GetExactValue()</div>
                            <span>Hours Requested</span>
                        }
                        else
                        {
                            <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;"></div>
                            <span>Hours Requested</span>
                        }
                    </td>
                </tr>
            </table>

        </div>
        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="72%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">
                            @if (@Model.Request_PreApprovedovertime)
                            {<span>Yes</span>}
                            else
                            {<span>No</span>}
                        </div>
                        Request for payment of Pre-Approved OvertimePaid(OT) Hours
                    </td>

                    <td width="28%" style="padding-right:10px;" valign="top">
                        @if (@Model.Request_PreApprovedovertime)
                        {
                            <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Hours_Requested_Overtime</div>
                            <span>Hours Requested</span>}
                        else
                        { <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;"></div>
                            <span>Hours Requested</span>}
                    </td>
                </tr>
            </table>

        </div>

        <div style="padding:0 0 5px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="72%" style="padding-right:10px;">
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">
                            @if (Model.Request_PreApprovedBonuses)
                            {<span>Yes</span>}
                            else
                            {<span>No</span>}
                    </div>
                    Request for payment of Pre-Approved Bonuses
                </td>

                <td width="28%" style="padding-right:10px;" valign="top">
                    @if (Model.Request_PreApprovedBonuses)
                    {
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Request_PreApprovedBonuses</div>
                        <span>Dolar Amount Requested</span>}
                    else
                    {
                        <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;"></div>
                        <span>Dolar Amount Requested</span>
                    }
                </td>
            </tr>
        </table>

    </div>
    <div style="padding:0 0 5px 0;">
        <table width="100%" border="0">
            <tr>
                <td width="72%" style="padding-right:10px;">
                    <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">
                        @if (Model.Request_PreApprovedMileage)
                        {<span>Yes</span>}
                        else
                        {<span>No</span>}
                </div>
                Request for payment of Pre-Approved Mileage
            </td>

            <td width="28%" style="padding-right:10px;" valign="top">
                @if (Model.Request_PreApprovedMileage)
                {
                    <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Miles_Requested</div>
                    <span>Miles Requested</span>}
                else
                {
                    <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;"></div>
                    <span>Miles Requested</span>
                }
            </td>
        </tr>
    </table>

</div>
<div style="padding:0 0 5px 0;">
    <table width="100%" border="0">
        <tr>
            <td width="72%" style="padding-right:10px;">
                <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@if (Model.Private_message_minutes != null)
                {<span>Yes</span>}
                else
                {<span>No</span>}</div>
                Request for payment of Private Message Minutes
            </td>

            <td width="28%" style="padding-right:10px;" valign="top">
                @if (Model.Private_message_minutes != null)
                {
                    <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;">@Model.Minutes_Requested</div>
                    <span>Minutes Requested</span>
                }
                else
                {
                    <div style="padding:8px 0 8px 0; border-bottom:1px solid #333; margin-bottom:5px; min-height:20px;"></div>
                    <span>Minutes Requested</span>
                }
            </td>
        </tr>
    </table>

</div>


</div>


</body>
</html>



















