                //$('input#radioChild_' + chkRadio[i]).attr('checked', 'true');
                //$(".PerformIndipendentWithAssist_" + chkRadio[i]).prop("checked", true);
                $("input[type=radio][name='radioChild_'" + chkRadio[i]).attr("checked", true);
