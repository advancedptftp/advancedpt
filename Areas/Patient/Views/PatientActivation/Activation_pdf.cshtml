@model DataLayer.Models.PatientActivation
@using DataLayer;
@{
    ViewBag.Title = "Activation_pdf";
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>myadvancedpt.com</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <link rel="shortcut icon" href="favicon.png">

    <!-- Bootstrap core CSS -->


</head>

<body>
    @using (Html.BeginForm("Create", "PatientActivation", FormMethod.Post, new { name = "Create", id = "Create" }))
    {

        <div style="width:95%; margin:0 auto; padding:40px 0; font-family:verdana; font-size:20px; color:#333;">


            <div style="padding: 35px 0 8px 0; font-size: 25px; font-weight: bold; font-size: 40px; border-bottom: 5px solid #231f20; color: #0c57a1; position: relative; color: #231f20;">

                <table width="100%" border="0" cellspacing="0" cellpadding="0">
                    <tr>
                        <td width="82%">Activation Form</td>
                    </tr>
                </table>




            </div>
            <div style="padding:35px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="7%" height="22">Date:</td>
                        <td width="21%"><div style="height:30px; border-bottom:1px solid #000; margin-bottom:7px; margin-right:7px;">@Model.Condition.Contact_Date.GetExactValue()</div></td>
                        <td width="12%">Staff Name:</td>
                        <td width="27%"><div style="height:30px; border-bottom:1px solid #000; margin-bottom:7px; margin-right:7px;">@Html.DisplayFor(m => m.User.Name, new { @class = "applitextfield" })</div></td>
                        <td width="15%">Contact&nbsp;Method:</td>
                        <td width="18%"><div style="height:30px; border-bottom:1px solid #000; margin-bottom:7px; margin-right:7px;">@(((PatientContactType)Enum.ToObject(typeof(PatientContactType), (Convert.ToInt32(Model.Condition.Contact_Type)))).ToCustomString())</div></td>
                    </tr>
                </table>

            </div>
            <div style="padding:20px 0 0 0; border-top:5px solid #231f20;"></div>            

            <div style="padding:15px 0 20px 0;  font-size:30px;"><strong>A. INTRODUCTION FOR ALL CALLS</strong></div>
            <div style="padding:8px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align:center; color:#000; font-weight:bold; vertical-align:top; padding-right:8px;">(A1)</td>
                        <td width="94%" valign="top">
                            Thank you for calling
                            <span style="width:500px; margin:0 10px 0 10px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                Advanced Physical Therapy Of Central Florida
                            </span>, how can I help you?
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:8px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align:center; color:#000; font-weight:bold; vertical-align:top; padding-right:8px;">(A2)</td>
                        <td width="94%" valign="top">
                            Have you been here before?
                            <strong>


                                @{
        if (Model.Condition.IsSeenHereBefore == 1)
        {
                        <label> Yes </label>
        }
        else
        {
                        <label> No </label>
        }
                                }



                            </strong>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A3)</td>
                        <td width="94%" valign="top">
                            May I have your name?
                            <span style="width:300px; margin:0 10px 0 10px;  display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @Html.DisplayFor(m => m.Patient.FirstName, new { @class = "applitextfield1" })
                            </span>
                            <span style="width:300px; margin:0 10px 0 10px;  display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">

                                @Html.DisplayFor(m => m.Patient.LastName, new { @class = "applitextfield1" })
                            </span>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A4)</td>
                        <td width="94%" valign="top">
                            What is the best phone number at which to reach you?
                            <span style="width:167px; margin:0 10px 0 10px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @{
                                if (Model.PatientUser.MobNo !=null)
                                {
                                    @Html.DisplayFor(m => m.PatientUser.MobNo, new { @class = "applitextfield1" })
                                }
                                else if (Model.Patient.Phone2 != null)
                                {
                                    @Html.DisplayFor(m => m.Patient.Phone2, new { @class = "applitextfield1" })
                                }
                                else if (Model.Patient.Phone3 != null)
                                {
                                    @Html.DisplayFor(m => m.Patient.Phone3, new { @class = "applitextfield1" })
                                }
                                }
                            </span>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A5)</td>
                        <td width="94%" valign="top">
                            And may I have your address?
                            <span style="width:700px; margin:0 10px 0 10px;  display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @{
                                     if(Model.Patient.AddressLine1!=null)
                                    {
                                    @Html.DisplayFor(m => m.Patient.AddressLine1, new { @class = "applitextfield1" })
                                    <span>,</span>
                                    }

                                    if(Model.Patient.AddressLine2!=null)
                                    {
                                    @Html.DisplayFor(m => m.Patient.AddressLine2, new { @class = "applitextfield1" })
                                    <span>,</span>
                                    }

                                    if(Model.Patient.City!=null)
                                    {
                                    @Html.DisplayFor(m => m.Patient.City, new { @class = "applitextfield1" })
                                    <span>,</span>
                                    }



                                    if (ViewBag.StateName!="")
                                    {
                                    @ViewBag.StateName<span>,</span>

                                   }
                                  if(Model.Patient.ZipCode!=null)
                                  {
                                    @Html.DisplayFor(m => m.Patient.ZipCode, new { @class = "applitextfield1" })
                                  }
                                }
                            </span>
                            <div style="padding-top:20px;"><strong>IF THEY ASK WHY:</strong> We want to get you to the office closest to your home.</div>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A6)</td>
                        <td width="94%" valign="top">
                            And may I have your birthdate as well?
                            <span style="width:200px; margin:0 10px 0 10px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @Model.Patient.DOB.GetExactValue()
                            </span>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A7)</td>
                        <td width="94%" valign="top">
                            Okay,
                            <span style="width:400px; margin:0 10px 0 10px;  display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @(Model.Patient.FirstName + " " + Model.Patient.LastName)
                            </span>
                            how did you hear about us?
                            <span style="width:200px; margin:0 10px 0 10px; padding-top:0px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @if (Model.Patient.tblMReferralSource != null)
                                {
                                    @Model.Patient.tblMReferralSource.Name
                                }

                            </span>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A8)</td>
                        <td width="94%" valign="top">
                            Very good, welcome to <span style="width:500px; margin:0 10px 0 10px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                Advanced Physical Therapy Of Central Florida
                            </span><br />my name is,
                            <span style="width:300px; margin:0 10px 0 10px; padding-top:0px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @Html.DisplayFor(m => m.User.Name, new { @class = "applitextfield1" })
                            </span>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" align="left" valign="top" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A9)</td>
                        <td width="94%" align="left" valign="top">
                            What exactly is wrong and how can we help you? <span style="width:200px;  display:inline-block; border-bottom:1px solid #000;">
                                @if (Model.Condition.ProblemType != 0 && Model.Condition.ProblemType != null)
                                {
                                    @(((ProblrmType)Enum.ToObject(typeof(ProblrmType), (Convert.ToInt32(Model.Condition.ProblemType)))).ToCustomString())
                                }
                            </span>
                            <span style="width:1100px; height:25px;  display:inline-block; border-bottom:1px solid #000; padding-top:20px;">@Html.DisplayFor(m => m.Condition.ProblemDetail)</span>
                            When did your @(((ProblrmType)Enum.ToObject(typeof(ProblrmType), (Convert.ToInt32(Model.Condition.ProblemType)))).ToCustomString()) happen ? @Convert.ToDateTime(Model.Condition.FromWhenProblemExist).GetExactValue()
                        </td>
                </tr>
            </table>

        </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" align="left" valign="top" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A10)</td>
                        <td width="94%" align="left" valign="top">
                            Are you experiencing a lot of @(((ProblrmType)Enum.ToObject(typeof(ProblrmType), (Convert.ToInt32(Model.Condition.ProblemType)))).ToCustomString()),
                            medium or a little bit? @(((ProblemStrength)Enum.ToObject(typeof(ProblemStrength), (Convert.ToInt32(Model.Condition.ProblemStrength)))).ToCustomString())
                        </td>
                    </tr>
                </table>

            </div>

            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" align="left" valign="top" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A11)</td>
                        <td width="94%" align="left" valign="top">
                            I’m sorry to hear you’re experiencing so much @(((ProblrmType)Enum.ToObject(typeof(ProblrmType), (Convert.ToInt32(Model.Condition.ProblemType)))).ToCustomString()).
                            We’ll try to get you in as
                            soon as possible.
                        </td>
                    </tr>
                </table>

            </div>
        <div style="padding:12px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A12)</td>
                    <td width="94%" valign="top">
                        Are you under the care of a physician? <strong>
                          

                                @{
                                    if (Model.Condition.IsUnderPhysician == 1)
                                    {
                                                                    <label> Yes </label>
                                    }
                                    else
                                    {
                                                                    <label> No </label>
                                    }
                                }                            
                        </strong>
                    </td>
                </tr>
            </table>

        </div>
        <div style="padding:12px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A13)</td>
                    <td width="94%" valign="top">
                        May I have his or her name please?
                        <span style="width:200px; margin:0 10px 0 10px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">

                            @ViewBag.DoctorName
                        </span>
                    </td>
                </tr>
            </table>

        </div>
        <div style="padding:12px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A14)</td>
                    <td width="94%" valign="top"><span style="width:1000px;">
    Do you know the name of your doctor’s group or their phone number?</span><br />
    <span style="width:200px; margin:0 10px 0 10px; padding-top:20px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
        @Html.DisplayFor(m => m.Condition.DoctorPhone, new { @class = "applitextfield1" })
    </span>
</td>
                </tr>
            </table>
        </div>
        <div style="padding:12px 0;">
            <table width="100%" border="0">
                <tr>
                    <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A15)</td>
                    <td width="94%" valign="top">
                        Now how do you intend to pay for your services? <span style="height: 30px; width: 200px; margin: 0 10px 0 10px; padding-top: 0px; display: inline-block; border-bottom: 1px solid #ccc; border-left: none; border-right: none; border-top: none;">
                            @(((PatientPaymentOption)Enum.ToObject(typeof(PatientPaymentOption), (Convert.ToInt32(Model.Condition.PaymentType)))).ToCustomString())
                        </span>
                    </td>
                </tr>
            </table>
        </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A16)</td>
                        <td width="94%" valign="top">
                            Are you seeking in home care or would you be coming to us? <span style="height: 30px; width: 200px; margin: 0 10px 0 10px; padding-top: 0px; display: inline-block; border-bottom: 1px solid #ccc; border-left: none; border-right: none; border-top: none;">
                                @{
                                    if (Model.Condition.SeekingCareType == 1)
                                    {
                                        <label>Home Care</label>
                                    }
                                    else
                                    {
                                        <label>Walk In</label>
                                    }
                                }
                            </span>
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A17)</td>
                        <td width="94%" valign="top">
                            Which office is most convenient? SW State Road 200 in Ocala, SE 17th Street in Ocala, Belleview, or The Villages?
                            <div style="padding-top:10px;">
                                First Choice



                                <span style="width:400px; margin:0 10px 0 10px; padding-top:0px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">

                                    @ViewBag.ConLoc1
                                </span>

                                Second Choice

                                <span style="width:300px; margin:0 10px 0 10px; padding-top:0px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                    @ViewBag.ConLoc2

                                </span>
                            </div>

                        </td>
                    </tr>
                </table>

            </div>

            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A18)</td>
                        <td width="94%" valign="top">
                            What day of the week works best for you?
                            <span style="width:200px; margin:0 10px 0 10px; padding-top:0px; display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                @(((SuitedDayOfWeek)Enum.ToObject(typeof(SuitedDayOfWeek), (Convert.ToInt32(Model.Condition.SuitedWeekDay)))).ToCustomString())
                            </span>

                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0 150px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px; ">(A19)</td>
                        <td width="94%" valign="top">
                            And are mornings or afternoons better? <strong>

                                @{
                                    if (Model.Condition.SuitedDayTime == 0)
                                    {
                                        <label> Anytime </label>
                                    }
                                    else if (Model.Condition.SuitedDayTime == 1)
                                    {
                                        <label> Mornings </label>
                                    }
                                    else
                                    {
                                        <label> Afternoons </label>
                                    }
                                }
                            </strong>
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A20)</td>
                        <td width="94%" valign="top">
                            Are you looking to get in as soon as possible or can you wait a few days? <strong>
                                <span class="p_left15">

                                    @{
                                        if (Model.Condition.CanWaitUpto == 0)
                                        {
                                            <label> Urgent </label>
                                        }
                                        else if (Model.Condition.CanWaitUpto == 1)
                                        {
                                            <label> Can Wait a Week </label>
                                        }
                                        else
                                        {
                                            <label> Can Wait a Month </label>
                                        }
                                    }
                                </span>
                            </strong>
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right:8px">(A21)</td>
                        <td width="94%" valign="top">
                            Okay, let me go ahead and check to see exactly what times we have available and I’ll be right back, okay? <strong>(Hold)</strong>
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px">(A22)</td>
                        <td width="94%" valign="top">
                            Hi
                            <span style="height: 30px; width: 200px; margin: 0 10px 0 10px; display: inline-block; border-bottom: 1px solid #ccc; border-left: none; border-right: none; border-top: none;">
                                @Html.DisplayFor(m => m.Patient.FirstName, new { @class = "applitextfield1" })
                            </span> are you still there? Great News! We have appointments available on
                            <strong>
                                <span style="height: 30px; width: 200px; margin: 0 10px 0 10px; padding-top: 0px; display: inline-block; border-bottom: 1px solid #ccc; border-left: none; border-right: none; border-top: none;">

                                    @ViewBag.DayName
                                </span>

                            </strong>, @Convert.ToDateTime(Model.Condition.ScheduledDateTime).GetExactValue(). Which one do you prefer?
                            <span style="height: 30px; width: 300px; margin: 0 10px 0 10px; padding-top: 0px; display: inline-block; border-bottom: 1px solid #ccc; border-left: none; border-right: none; border-top: none;">
                                @Convert.ToDateTime(Model.Condition.ScheduledDateTime).GetExactValue()
                            </span>
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(A23)</td>
                        <td width="94%" valign="top">
                            Okay, I’m going to go ahead and put you down for
                            <span style="width: 300px; margin: 0 10px 0 10px; display:inline-block; border-bottom: 1px solid #ccc; border-left: none; border-right: none; border-top: none;">
                                @Convert.ToDateTime(Model.Condition.ScheduledDateTime).GetExactValue()
                            </span>
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px">(A24)</td>
                        <td width="94%" valign="top">
                            Now I’m going to reserve this spot for you, so if for any reason you might have to cancel or reschedule, call us at
                            least 48 hours in advance to let us know, okay?
                        </td>
                    </tr>
                </table>

            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px">(A25)</td>
                        <td width="94%" valign="top">
                            Do you have any concerns with transportation?
                            @{
                                    if (Model.Condition.IsTransport == 1)
                                    {
                                                    <label> Yes </label>
                                    }
                                    else
                                    {
                                                    <label> No </label>
                                    }
                            }
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px">(A26)</td>
                        <td width="94%" valign="top">
                            What county do you live in? @Model.Condition.County.GetExactValue()
                        </td>
                    </tr>
                </table>
            </div>
            <div style="padding:12px 0;">
                <table width="100%" border="0">
                    <tr>
                        <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px">(A27)</td>
                        <td width="94%" valign="top">
                            Okay, after we hang up I will contact @Model.Condition.County.GetExactValue() County transport and arrange for your transportation.
                        </td>
                    </tr>
                </table>
            </div>
            @if (Model.Condition.SeekingCareType == ((Convert.ToInt32(SeekingCareType.Home_Care)) + 1))
            {

                <div style="padding:30px 0 20px 0; font-size:25px;"><strong>B. FOR HOME HEALTHCARE ONLY</strong></div>
                <div style="padding:12px 0;">
                    <table width="100%" border="0">
                        <tr>
                            <td width="6%" style="text-align: center; color: #000; font-weight: bold; vertical-align: top; padding-right: 8px;">(B1)</td>
                            <td width="94%" valign="top">
                                Okay, what city do you live in?
                                <span style="width:200px; margin:0 10px 0 10px;  display:inline-block; border-bottom:1px solid #ccc; border-left:none; border-right:none; border-top:none;">
                                    @Html.DisplayFor(m => m.Patient.City)
                                </span>
                            </td>
                        </tr>
                    </table>

                </div>
